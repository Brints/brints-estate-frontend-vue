    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.37",
    "rollup": "^4.17.2",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.4.0",
    "vite": "^5.1.5",
